{
  "entryFile": "src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "src/**/*controller.ts"
  ],
  "spec": {
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "authentication",
        "in": "header"
      }
    },
    "outputDirectory": "src",
    "specVersion": 3
  },
  "routes": {
    "authenticationModule": "src/security/authentication.ts",
    "routesDir": "src/routes"
  }
}